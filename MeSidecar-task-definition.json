{
    "containerDefinitions": [
        {
            "name": "microenforcer",
            "image": "633291361733.dkr.ecr.us-west-1.amazonaws.com/andreasm:microenforcer-basic",
            "cpu": 0,
            "portMappings": [],
            "essential": false,
            "environment": [],
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [],
            "ulimits": []
        },
        {
            "name": "nginx",
            "image": "633291361733.dkr.ecr.us-west-1.amazonaws.com/andreasm:nginx",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "nginx-80-tcp",
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "entryPoint": [
                "/.aquasec/bin/microenforcer"
            ],
            "command": [
                "nginx",
                "-g",
                "daemon off;"
            ],
            "environment": [
                {
                    "name": "AQUA_TOKEN",
                    "value": "edc5deb6-828c-4cc9-82b0-8621212a697f"
                },
                {
                    "name": "AQUA_DEBUG_LEVEL",
                    "value": "3"
                },
                {
                    "name": "AQUA_DEBUG_TYPE",
                    "value": "STDOUT"
                },
                {
                    "name": "AQUA_MICROENFORCER",
                    "value": "1"
                },
                {
                    "name": "AQUA_IMAGE_ID_OLD",
                    "value": "sha256:b690f5f0a2d535cee5e08631aa508fef339c43bb91d5b1f7d77a1a05cea021ab"
                },
                {
                    "name": "LD_PRELOAD",
                    "value": "/.aquasec/bin/$PLATFORM/slklib.so"
                },
                {
                    "name": "https_proxy",
                    "value": "http://20.79.99.101:80"
                },
                {
                    "name": "http_proxy",
                    "value": "http://20.79.99.101:80"
                },
                {
                    "name": "no_proxy",
                    "value": "169.254.170.2,169.254.169.254"
                },
                {
                    "name": "AQUA_SERVER",
                    "value": "c1fae5dbe2-gw.cloud.aquasec.com:443"
                }
            ],
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [
                {
                    "sourceContainer": "microenforcer",
                    "readOnly": false
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-create-group": "true",
                    "awslogs-group": "/ecs/AndreasMmeSidecar",
                    "awslogs-region": "us-west-1",
                    "awslogs-stream-prefix": "ecs"
                },
                "secretOptions": []
            }
        }
    ],
    "family": "AndreasM-ME-Sidecar",
    "taskRoleArn": "arn:aws:iam::633291361733:role/ecsTaskExecutionRole",
    "executionRoleArn": "arn:aws:iam::633291361733:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "512",
    "memory": "1024",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    },
    "tags": []
}
